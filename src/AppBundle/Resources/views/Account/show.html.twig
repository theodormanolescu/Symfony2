    {% extends 'common/app.html.twig' %}
    {% block header%}
    <h1>Account</h1>
    {% endblock %}
    {% block content%}
    <table class="record_properties">
        <tbody>            <tr>
                <th>Id</th>                    <td>
                        {{ entity.id }}
                    </td>            </tr>            <tr>
                <th>Login</th>                    <td>
                        {{ entity.login }}
                    </td>            </tr>            <tr>
                <th>Password</th>                    <td>
                        {{ entity.password }}
                    </td>            </tr>            <tr>
                <th>Email</th>                    <td>
                        {{ entity.email }}
                    </td>            </tr>            <tr>
                <th>Active</th>                    <td>
                        {{ entity.active }}
                    </td>            </tr>        </tbody>
    </table>
        <ul class="record_actions">
    <li>
        <a href="{{ path('account') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('account_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
    {% endblock %}
