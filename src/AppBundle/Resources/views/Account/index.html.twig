    {% extends 'common/app.html.twig' %}
    {% block header%}<h1>Account list</h1>{% endblock %}
    {% block content%}
    <table class="records_list">
        <thead>
        <tr>                <th>Id</th>                <th>Login</th>                <th>Password</th>                <th>Email</th>                <th>Active</th>            <th>Actions</th>
        </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
        <tr>                    <td>
                        <a href="{{ path('account_show', { 'id': entity.id }) }}">
                            {{ entity.id }}</a>
                    </td>                    <td>{{ entity.login }}</td>                    <td>{{ entity.password }}</td>                    <td>{{ entity.email }}</td>                    <td>{{ entity.active }}</td>                    <td>
                <ul>
                    <li>
                        <a href="{{ path('account_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('account_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                    </td>        </tr>
        {% endfor %}
        </tbody>
    </table>
            <a class="button" href="{{ path('account_new') }}">
            New Account
        </a>
        {% endblock %}
