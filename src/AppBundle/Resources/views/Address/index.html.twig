    {% extends 'common/app.html.twig' %}
    {% block header%}<h1>Address list</h1>{% endblock %}
    {% block content%}
    <table class="records_list">
        <thead>
        <tr>                <th>Id</th>                <th>Name</th>                <th>City</th>                <th>Streetname</th>                <th>Streetnumber</th>                <th>Building</th>                <th>Entrance</th>                <th>Number</th>                <th>Deleted</th>            <th>Actions</th>
        </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
        <tr>                    <td>
                        <a href="{{ path('address_show', { 'id': entity.id }) }}">
                            {{ entity.id }}</a>
                    </td>                    <td>{{ entity.name }}</td>                    <td>{{ entity.city }}</td>                    <td>{{ entity.streetName }}</td>                    <td>{{ entity.streetNumber }}</td>                    <td>{{ entity.building }}</td>                    <td>{{ entity.entrance }}</td>                    <td>{{ entity.number }}</td>                    <td>{{ entity.deleted }}</td>                    <td>
                <ul>
                    <li>
                        <a href="{{ path('address_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('address_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                    </td>        </tr>
        {% endfor %}
        </tbody>
    </table>
            <a class="button" href="{{ path('address_new') }}">
            New Address
        </a>
        {% endblock %}
