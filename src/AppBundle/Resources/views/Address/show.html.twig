    {% extends 'common/app.html.twig' %}

    {% block header%}
    <h1>Address</h1>
    {% endblock %}

    {% block content%}

    <table class="record_properties">
        <tbody>            <tr>
                <th>Id</th>                <td>
                    {{ entity.id }}
                </td>            </tr>            <tr>
                <th>Name</th>                <td>
                    {{ entity.name }}
                </td>            </tr>            <tr>
                <th>City</th>                <td>
                    {{ entity.city }}
                </td>            </tr>            <tr>
                <th>Streetname</th>                <td>
                    {{ entity.streetName }}
                </td>            </tr>            <tr>
                <th>Streetnumber</th>                <td>
                    {{ entity.streetNumber }}
                </td>            </tr>            <tr>
                <th>Building</th>                <td>
                    {{ entity.building }}
                </td>            </tr>            <tr>
                <th>Entrance</th>                <td>
                    {{ entity.entrance }}
                </td>            </tr>            <tr>
                <th>Number</th>                <td>
                    {{ entity.number }}
                </td>            </tr>            <tr>
                <th>Deleted</th>                <td>
                    {{ entity.deleted }}
                </td>            </tr>        </tbody>
    </table>

        <div class="record-actions">
    <a class="button" href="{{ path('address') }}">
        Back to the list
    </a>
            <a class="button" href="{{ path('address_edit', 
            { 'id': entity.id }) }}">
            Edit
        </a>
                <div style="float:left">
        {{ form(delete_form) }}
        </div>
        <div style="clear:both"></div>
    </div>
    {% endblock %}
