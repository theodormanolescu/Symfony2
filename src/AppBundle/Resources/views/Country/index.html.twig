    {% extends 'common/app.html.twig' %}

    {% block header%}
    <h1>Country list</h1>
    {% endblock %}

    {% block content%}
    <table class="records_list">
        <thead>
            <tr>                <th>Id</th>                <th>Code</th>                <th>Name</th>                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
            {% for entity in entities %}
            <tr>                <td>
                    <a href="{{ path('country_show', { 'id': entity.id }) }}">
                        {{ entity.id }}
                    </a>
                </td>                <td>{{ entity.code }}</td>                <td>{{ entity.name }}</td>                <td><div class="actions">    <a class="button" href="{{ path('country_show', { 'id': entity.id }) }}">
        show
    </a>    <a class="button" href="{{ path('country_edit', { 'id': entity.id }) }}">
        edit
    </a></div>
                </td>            </tr>
            {% endfor %}
        </tbody>
    </table>

            <a class="button" href="{{ path('country_new') }}">
            New Country
        </a>
        {% endblock %}
