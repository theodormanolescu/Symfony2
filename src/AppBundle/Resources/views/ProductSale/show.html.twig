    {% extends 'common/app.html.twig' %}
    {% block header%}
    <h1>ProductSale</h1>
    {% endblock %}
    {% block content%}
    <table class="record_properties">
        <tbody>            <tr>
                <th>Id</th>                    <td>
                        {{ entity.id }}
                    </td>            </tr>            <tr>
                <th>Active</th>                    <td>
                        {{ entity.active }}
                    </td>            </tr>            <tr>
                <th>Startdate</th>                    <td>{{ entity.startDate|date('Y-m-d H:i:s') }}</td>            </tr>            <tr>
                <th>Enddate</th>                    <td>{{ entity.endDate|date('Y-m-d H:i:s') }}</td>            </tr>            <tr>
                <th>Price</th>                    <td>
                        {{ entity.price }}
                    </td>            </tr>        </tbody>
    </table>
        <ul class="record_actions">
    <li>
        <a href="{{ path('productsale') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('productsale_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
    {% endblock %}
