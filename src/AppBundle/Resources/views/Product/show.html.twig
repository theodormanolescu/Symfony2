    {% extends 'common/app.html.twig' %}
    {% block header%}
    <h1>Product</h1>
    {% endblock %}
    {% block content%}
    <table class="record_properties">
        <tbody>            <tr>
                <th>Id</th>                    <td>
                        {{ entity.id }}
                    </td>            </tr>            <tr>
                <th>Code</th>                    <td>
                        {{ entity.code }}
                    </td>            </tr>            <tr>
                <th>Title</th>                    <td>
                        {{ entity.title }}
                    </td>            </tr>            <tr>
                <th>Description</th>                    <td>
                        {{ entity.description }}
                    </td>            </tr>        </tbody>
    </table>
        <ul class="record_actions">
    <li>
        <a href="{{ path('product') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('product_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
    {% endblock %}
